{
  "require": {
    "squizlabs/php_codesniffer": "^3.6.2",
    "slevomat/coding-standard": "~7.0",
    "pheromone/phpcs-security-audit": "*",
    "magento/magento-coding-standard": "~31.0",
    "wp-coding-standards/wpcs": "*",
    "phpcompatibility/php-compatibility": "*",
    "phpcompatibility/phpcompatibility-wp": "*",
    "phpcompatibility/phpcompatibility-paragonie": "*",
    "magento/marketplace-eqp": "*",
    "drupal/coder": "*",
    "automattic/vipwpcs": "*",
    "escapestudios/symfony2-coding-standard": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "symfony/flex": "*",
    "doctrine/coding-standard": "*",
    "vaimo/phpcs-rulesets": "^1.3.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/magento/marketplace-eqp.git"
    },
    {
      "type": "vcs",
      "url": "https://bitbucket.org/vaimo/phpcs-rulesets.git"
    }


  ],
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "symfony/flex": true,
      "phpdocumentor/unified-asset-installer": true
    }
  }
}
