{
    "require": {
        "squizlabs/php_codesniffer": "^3.7.2",
        "slevomat/coding-standard": "^8.13.4",
        "pheromone/phpcs-security-audit": "^2.0.1",
        "magento/magento-coding-standard": "v31",
        "wp-coding-standards/wpcs": "^2.3",
        "phpcompatibility/php-compatibility": "^9.3.5",
        "phpcompatibility/phpcompatibility-wp": "^2.1.4",
        "phpcompatibility/phpcompatibility-paragonie": "^1.3.2",
        "magento/marketplace-eqp": "^4.0.0",
        "drupal/coder": "^8.3.21",
        "automattic/vipwpcs": "^2.3.4",
        "escapestudios/symfony2-coding-standard": "^3.13.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0.0",
        "symfony/flex": "v1.20.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/magento/marketplace-eqp.git"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/flex": true,
            "phpdocumentor/unified-asset-installer": true
        }
    }
}
