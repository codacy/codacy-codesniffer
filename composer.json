{
    "require": {
        "squizlabs/php_codesniffer": "3.7.2",
        "slevomat/coding-standard": "7.2.0",
        "pheromone/phpcs-security-audit": "2.0.1",
        "magento/magento-coding-standard": "v6",
        "wp-coding-standards/wpcs": "2.3.0",
        "phpcompatibility/php-compatibility": "9.3.5",
        "phpcompatibility/phpcompatibility-wp": "2.1.4",
        "phpcompatibility/phpcompatibility-paragonie": "1.3.2",
        "magento/marketplace-eqp": "4.0.0",
        "drupal/coder": "8.3.15",
        "automattic/vipwpcs": "2.3.3",
        "escapestudios/symfony2-coding-standard": "^3.11",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/magento/marketplace-eqp.git"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
